print("Welcome to my number list generator.")
print()
print("You are going to give me a number you want to start with, an ending number, and by how many you want me to add each time.")
print()
start = int(input("What number do you want to start with? "))
l = int(input("What number do you want to end with? "))
last = l+1
increment = int(input("How many should I add each time? "))
print()
for i in range(start, last, increment):
  print(i)
